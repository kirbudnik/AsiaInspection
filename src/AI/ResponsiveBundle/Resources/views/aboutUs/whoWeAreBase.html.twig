{% extends 'AIResponsiveBundle:aboutUs:aboutUsBase.html.twig' %}
{% block title %}{% if pageTitle is defined %}{{pageTitle}}{% endif %}{% endblock %}
{% block pageDescription %}{% if pageDesc is defined %}{{pageDesc}}{% endif %}{% endblock %}

{% block pagename %}WhoWeAre{% endblock %}

{% block aboutUsMainBlock %}
    <div id="aboutUs" class="col-xs-12 col-sm-offset-1 col-sm-6 col-md-offset-1 col-md-7 col-lg-7 col-xl-offset-0 col-xl-8 sidebox  paddingLeftZero" style=" margin-bottom:20px;margin-top:20px;">
        {% block whoWeAreText %}{% endblock %}
    </div>
    <div class="sidebox col-xs-offset-1 col-xs-10 col-sm-offset-0  col-sm-5 col-md-4 col-md-offset-0 col-lg-4 col-lg-offset-0" style="margin-top:20px;">
        <div class="sidebox col-xs-12 col-md-offset-1 ">
            <div class="offer-box pDeepGrey " >
                {% block offerbox1 %}{% endblock %}
            </div>
            {% block imagebox %}{% endblock %}
            <div class="offer-box pDeepGrey" style="margin-top:20px;">
                {% block offerbox2 %}{% endblock %}
            </div>
            {% block offerbox3 %}{% endblock %}
        </div>
    </div>
{% endblock %}
