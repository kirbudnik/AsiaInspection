    <style>
        * { box-sizing: border-box; }

        .tabContainer{
            border:0px;
            margin-bottom:0px;
        }

        .serviceTab{
            background-color:white;
            border:0px;
            border-bottom: 3px solid #dadada;
        }

        .selectedServiceTab{
            background-color:white;
            color:#b31820;
            border-bottom:3px solid #b31820;
        }
    
        .panorama {
            position: relative;
            width: 100%;
            height: 500px;
            display:inline-block;
            overflow:hidden;
            cursor: move; /* fallback if grab cursor is unsupported */
            cursor: grab;
            cursor: -moz-grab;
            cursor: -webkit-grab;
        }

        .panorama:active { 
            cursor: grabbing;
            cursor: -moz-grabbing;
            cursor: -webkit-grabbing;
        }
    
        .hotspot {
            position:absolute;
            display:block;
            width:25px;
            cursor:pointer;
            border-radius:22px;
            z-index:4;
        }
    
        .hotspot:hover{
            -webkit-box-shadow: 0px 0px 10px 10px rgba(255, 255, 255, 0.65);
            -moz-box-shadow: 0px 0px 10px 10px rgba(255, 255, 255, 0.65);
            box-shadow: 0px 0px 10px 10px rgba(255, 255, 255, 0.65);
            transition: all 150ms ease-out;
        }
    
        .panoramaImage {
            position:absolute;
            background: url(images/360labs/buffalo-panorama.jpg) repeat;
            background-size: cover;
            background-position: 0px 0px;
            width: 100%;
            height: 500px;
        }
    
        .panoramaOverlay {
            background-color:rgba(0,0,0,0.3);
            width: 100%;
            height: 100%;
            position:absolute;
            border:1px solid black;
        }
    
        .panoramaWindow {
            position:absolute;
            margin: 0px;
            width: 100%;
            height: 100%;
            overflow:hidden;
        }

        .popupOverlay {
            position:absolute;
            background-color: rgba(0,0,0,0.7);
            margin: 0;
            width: 100%;
            height: 0px;
            z-index:5;
            opacity:0;
            display:none;
            cursor:pointer;
        }

        .popupSubWrapper {
            position:relative;
            margin:5% 2.5%;
            background-color:white;
            border:1px solid black;
            width:95%;
            max-height:85%;
            border-radius:3px;
            /* overflow:hidden; */
            padding:15px;
        }

        .panoramaHeaderContent {
            margin:15px;
        }

        .accredIcon {
            margin:5px;
            vertical-align: middle;
        }
        .backToTourButtonBox {
            padding:10px;
            border-radius:3px;
            background-color:#5C9DD1;
            color:white;
            font-weight:bold;
            display:inline-block;
            margin-top:10px;
        }

        .labNavButtonWrapper {
            text-align:center;
            display:inline-block;
            vertical-align:top;
        }
        .labNavButtonContainer {
            margin:15px 0px;
            float:left;
        }
    </style>
    
