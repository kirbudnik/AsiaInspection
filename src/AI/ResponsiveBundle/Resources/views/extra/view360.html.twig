    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.js"></script>
    
    <script type="text/javascript">
        PlaceMode = false;
        panoramaStop = false;

        // Safari Detection
        Safari = false;
        var lastX;
        if (navigator.userAgent.indexOf('Safari') != -1 && navigator.userAgent.indexOf('Chrome') == -1) { Safari = true; }

        // Hotspot Placer [Begin]
        $(document).keydown(function(e) {
            //O (79) - Enable Placement Mode
            //K (75) - Disable Placement Mode and Dump Coordinates to Console
            key = e.keyCode;
            if( key == 79 ) {
                PlaceMode = true;
                $('.hotspot').each(function() {
                    $(this).draggable({
                        containment: 'parent',
                        disabled: false
                    });
                    $(this).append("<div class='dragIdentify' style='position:absolute; top:0px; left:0px; background-color:yellow; font-size:10px;'>" + $(this).attr("machine") + "</div>");
                });
                console.log("----- Placement Mode Enabled -----");
            }

            if( key == 75 ) {
                PlaceMode = false;
                $('.hotspot').each(function() {
                    $(this).draggable('disable');
                });
                $('.dragIdentify').remove();
                console.log("----- Placement Mode Disabled -----");
                parentPanorama = $(".panorama:visible");

                backgroundPos = $(".panoramaImage", parentPanorama).css('backgroundPosition').split(" ");
                xPos = parseInt(backgroundPos[0]);
                xPos = -1 * xPos;

                $(".hotspot", parentPanorama).each(function() {
                    lft = parseInt($(this).css("left"));
                    if(lft > (0-iconWidth) && lft < viewWidth) { // if it's on screen
                        machine = $(this).attr("machine");
                        x = Math.round(lft + xPos) ;
                        y = Math.round($(this).position().top);
                        console.log("["+machine+"] top:"+y+"px; left:"+x+"px;");
                    }
                });
            }
       });
        // Hotspot Placer [End]

        viewWidth = parseFloat($(".panoramaWindow:visible").css("width"));
        iconWidth = parseFloat($(".hotspot").eq(0).css("width"));

        resize360 = function() {
            viewWidth = parseFloat($(".panoramaWindow:visible").css("width"));
        }
        $(window).load(resize360);
        $(window).resize(resize360);

        $(".panoramaWindow").mousedown(function (e) {
            parentPanorama = $(this).closest(".panorama:visible");
            imageWidth = parseInt($(".panoramaImage", parentPanorama).attr("imgwidth")); // width of background image
            lastX = e.clientX;
            $(this).mousemove(function (e) {
                panoramaStop = true;
                if(Safari) {
                    mod = e.clientX - lastX;
                    lastX = e.clientX;
                } else {
                    mod = e.originalEvent.movementX;    
                }

                direction = (mod >= 0 ? "right" : "left");
                // Handle Hotspots
                $(this).find(".hotspot").each(function() {
                    x = $(this).position().left + mod;
                    // If it goes out of view on the left
                    if(x < (0-iconWidth) && direction == "left") {
                        x = x + imageWidth;
                    }
                    // If it goes out of view on the right
                    if(x > viewWidth && direction == "right" && mod != 0) {
                        x = x - imageWidth;
                    }
                    $(this).css("left",x+"px");
                });

                // Handle Background Image
                backgroundPos = $(".panoramaImage", parentPanorama).css('backgroundPosition').split(" ");
                xPos = parseInt(backgroundPos[0]);
                $(".panoramaImage", parentPanorama).css('backgroundPosition',xPos+mod+"px 0px");

            });
        }).mouseup(function () {
            $(this).unbind('mousemove');
        }).mouseout(function () {
            $(this).unbind('mousemove');
        });

        $(".hotspot").click(function() {
            if( !PlaceMode ) {
                parentPanorama = $(this).closest(".panorama:visible");
                id = $(this).attr("machine");
                pop = $(".popupOverlay", parentPanorama);
                $(".machineImage", pop).attr("src", popups[id].image.src);
                $(".machineTitle", pop).html(popups[id].title);
                $(".machineContent", pop).html(popups[id].content);
                $(pop).show().animate({
                    height: "100%",
                    opacity: 1
                }, 300);
            }
        });

        $(".popupOverlay").click(function(){
            $(this).animate({
                height: "0px",
                opacity: 0
            }, 300, function() {
                $(this).hide();
            });
        });

        // To move the 360 view if they haven't moved it by themselves yet
        panoramaShifter = setInterval(autoScrollPanorama, 50);
        $(".hotspot").mouseover(function (e) { panoramaStop = true; });
        function autoScrollPanorama() {
            if(!panoramaStop) {

                direction = "left";
                mod = 2;
                parentPanorama = $(".panorama:visible");
                imageWidth = parseInt($(".panoramaImage", parentPanorama).attr("imgwidth")); // width of background image

                // Handle Hotspots
                $(".panoramaWindow", parentPanorama).find(".hotspot").each(function() {
                    x = $(this).position().left + mod;
                    // If it goes out of view on the right
                    if(x > viewWidth) {
                        x = x - imageWidth;
                    }
                    $(this).css("left",x+"px");
                });

                // Handle Background Image
                backgroundPos = $(".panoramaImage", parentPanorama).css('backgroundPosition').split(" ");
                xPos = parseInt(backgroundPos[0]);
                $(".panoramaImage", parentPanorama).css('backgroundPosition',xPos+mod+"px 0px");

            } else {
                clearInterval(panoramaShifter);
            }
        }

        //$(".labNavButtonWrapper").click(function(){
        //    $(".blueDownCaret:visible").remove();
        //    $(this).append("<img class='blueDownCaret' src='/images/blue-down-caret.png' />")
        //});

        //selecting different tabs when clicking on them on the service page
        $(".labViewButton").click(function() {
            target = $(this).attr("tab");
            $(".tabContent").css("display","none");
            $("#tabContent_"+target).css("display","block");
        });

        var popups = {};

        popups.multiExtractionHeatingMantle = {};
        popups.multiExtractionHeatingMantle.image = "images/360labs/equipment1.jpg";
        popups.multiExtractionHeatingMantle.title = "Multi Extraction Heating Mantle";
        popups.multiExtractionHeatingMantle.content = "<p>Used to apply heat to boiling flasks in a controlled and safe manner. This unit is used to reflux (boil) materials in solvent for Indirect Food Additive testing. The condensers are affixed to the top of each boiling flask to collect solvent vapors and direct them back into the boiling flask.</p>";

        popups.ultrasonicBath = {};
        popups.ultrasonicBath.image = "images/360labs/equipment2.jpg";
        popups.ultrasonicBath.title = "Ultrasonic Bath";
        popups.ultrasonicBath.content = "<p>Uses high frequency pressure (sound) waves to produce cavitation bubbles which can act directly on materials in the bath. The agitation produces high forces on materials which, depending on the liquids used, may act as an aid for cleaning or as used here, to dissolve materials in a solvent. This unit is used to aid the dissolution of materials into solvent for the analysis of phthalates.</p>";

        popups.reciprocatingShakingWaterBath = {};
        popups.reciprocatingShakingWaterBath.image = "images/360labs/equipment3.jpg";
        popups.reciprocatingShakingWaterBath.title = "Reciprocating Shaking Water Bath";
        popups.reciprocatingShakingWaterBath.content = "<p>Water baths are particularly useful for controlling the temperature of test samples in the lab. These units have variable speed reciprocating platforms that can allow for constant movement of samples when required. These water baths are used for the extraction of samples mimicking normal body temperature.  Additionally they are used for extraction and controlled cooling of Indirect Food Additive samples at a variety of temperatures.</p>";

        popups.analyticalBalance = {};
        popups.analyticalBalance.image = "images/360labs/equipment4.jpg";
        popups.analyticalBalance.title = "Analytical Balance";
        popups.analyticalBalance.content = "<p>A class of balance designed to measure mass down to the sub-milligram range. The measuring pan of this analytical balance is inside a transparent enclosure with doors so that dust does not collect and so any air currents in the room do not affect the balance's operation. This type of balance is used for nearly all tests performed in the lab that require the measurement of mass. This particular unit may also be set up with a device that allows for the determination of sample density. </p>";

        popups.hotBlockProDigestionSystem = {};
        popups.hotBlockProDigestionSystem.image = "images/360labs/equipment5.jpg";
        popups.hotBlockProDigestionSystem.title = "Hot Block Pro Digestion System";
        popups.hotBlockProDigestionSystem.content = "<p>The HotBlock is an efficient way to heat multiple samples with accurate temperature control and no cold spots. Hot Blocks use low cost, disposable, certified digestion vessels that speed and simplify digestion procedures. These vessels may also be used for volume measurement, can accept a unique filter and can be used as sample storage containers. They eliminate the need for glassware and glassware cleanup and therefore eliminate possible sample carryover. Digestion vessels contain no detectable background contaminants.</p>";

        popups.marsExpress = {};
        popups.marsExpress.image = "images/360labs/equipment6.jpg";
        popups.marsExpress.title = "MARS Express – Microwave Accelerated Reaction System";
        popups.marsExpress.content = "<p>The microwave accelerated reaction system is designed for digesting, dissolving, hydrolyzing a wide variety of materials in a laboratory setting. The system uses microwave energy to heat samples in acid solutions rapidly and at elevated pressures. This particular unit is set up to monitor the temperature of each sample during the digestion to provide feedback that allows for varying the amount of microwave energy to keep the temperature within preset conditions. Its main purpose is for preparing samples for analysis by inductively coupled plasma optical emission spectroscopy (ICP-OES).</p>";

        popups.deionizedWaterSystem = {};
        popups.deionizedWaterSystem.image = "images/360labs/equipment7.jpg";
        popups.deionizedWaterSystem.title = "Deionized water System – RO/DI";
        popups.deionizedWaterSystem.content = "<p>Uses reverse osmosis purification technology which employs a semipermeable membrane to remove ions, molecules, and larger particles from tap water. This purified water is stored and available for final treatment using ion exchange resins to produce Type 1 reagent grade water.</p>";

        popups.scatteredLight = {};
        popups.scatteredLight.image = "images/360labs/scattered-light-tester.jpg";
        popups.scatteredLight.title = "Scattered light tester";
        popups.scatteredLight.content = "<p>According to the eyewear testing requirement, To measure the reduced luminous coefficient of the filters to see how much the light scattered from the test filters</p>";
        
        popups.refractivePower = {};
        popups.refractivePower.image = "images/360labs/img_refractivePower.jpg";
        popups.refractivePower.title = "Refractive power tester";
        popups.refractivePower.content = "<p>To test the spherical power, astigmatic power and prismatic powers of sun filters if there are any powers induced from production and if the power readings are less than the requirement</p>";
        
        popups.prismaticPower = {};
        popups.prismaticPower.image = "images/360labs/img_prismaticPower.jpg";
        popups.prismaticPower.title = "Prismatic power tester";
        popups.prismaticPower.content = "<p>To test the spherical power, astigmatic power and prismatic powers of sun filters if there are any powers induced from production and if the power readings are less than the requirement</p>";
        
        popups.highVelocityTester = {};
        popups.highVelocityTester.image = "images/360labs/img_highVelocityTester.jpg";
        popups.highVelocityTester.title = "High velocity testers";
        popups.highVelocityTester.content = "<p>To perform impact resistance test on PPE by placing the test sample inside the tester, adjust the required test speed to shot the steel ball on sample.</p>";
        
        popups.videoDigitalMeasurementTester = {};
        popups.videoDigitalMeasurementTester.image = "images/360labs/img_videoDigitalMeasurementTester.jpg";
        popups.videoDigitalMeasurementTester.title = "Video digital measurement tester";
        popups.videoDigitalMeasurementTester.content = "<p>To perform the size measurement of very small components by projected size of parts on the screen.</p>";
        
        popups.FridgeChemicalStorage = {};
        popups.FridgeChemicalStorage.image = "images/360labs/img_FridgeChemicalStorage.jpg";
        popups.FridgeChemicalStorage.title = "Refrigerator for chemical storage ";
        popups.FridgeChemicalStorage.content = "<p>Used to store some chemicals with specific storage environment such as low temperature before their expiry date.</p>";
        
        popups.ftir = {};
        popups.ftir.image = "images/360labs/img_ftir.jpg";
        popups.ftir.title = "FTIR";
        popups.ftir.content = "<p>Named as Fourier transform infrared spectroscopy to obtain an infrared spectrum of absorption or emission of a solid, liquid or gas. To compare with a database with specific spectrum for different kind of materials, the material can be identified.</p>";
        
        popups.gcMs = {};
        popups.gcMs.image = "images/360labs/img_gcMs.jpg";
        popups.gcMs.title = "GC-MS";
        popups.gcMs.content = "<p>Named as Gas chromatography-mass spectrometry (GC-MS) by combining the features of gas-chromatography and mass spectrometry to identify different substances within a specimen. Organic substances such as phthalates, organotin, polycyclic aromatic hydrocarbons (PAH) and etc. can be detected.</p>";
        
        popups.plc = {};
        popups.plc.image = "images/360labs/img_plc.jpg";
        popups.plc.title = "PLC";
        popups.plc.content = "<p>Named as high-performance liquid chromatography (HPLC) to separate, identify, and quantify each component in a mixture. It relies on pumps to pass a pressurized liquid solvent containing the sample mixture through a column filled with a solid adsorbent material. Different components cause different flow rates with the absorbent material and lead to be separated. It is commonly used for detecting AZO dyes from textile materials.</p>";
        
        popups.hplc = {};
        popups.hplc.image = "images/360labs/img_hplc.jpg";
        popups.hplc.title = "HPLC";
        popups.hplc.content = "<p>Named as high-performance liquid chromatography (HPLC) to separate, identify, and quantify each component in a mixture. It relies on pumps to pass a pressurized liquid solvent containing the sample mixture through a column filled with a solid adsorbent material. Different components cause different flow rates with the absorbent material and lead to be separated. It is commonly used for detecting AZO dyes from textile materials.</p>";
        
        popups.gcMsHeadspace = {};
        popups.gcMsHeadspace.image = "images/360labs/img_gcMsHeadspace.jpg";
        popups.gcMsHeadspace.title = "GC-MS with headspace";
        popups.gcMsHeadspace.content = "<p>Named as Gas chromatography-mass spectrometry (GC-MS) by combining the features of gas-chromatography and mass spectrometry to identify different substances within a specimen. The headspace is used to analyze volatile compounds. This set-up is commonly used to evaluate food grade plastics.</p>";
        
        popups.polarizedAxisOne = {};
        popups.polarizedAxisOne.image = "images/360labs/polarized-axis-tester.jpg";
        popups.polarizedAxisOne.title = "Polarized axis tester";
        popups.polarizedAxisOne.content = "<p>To test the polarization axis of a pair of polarized lenses</p>";
        
        popups.laserReferencePoint = {};
        popups.laserReferencePoint.image = "images/360labs/laser-reference-point.jpg";
        popups.laserReferencePoint.title = "Laser reference point tester";
        popups.laserReferencePoint.content = "<p>To determine the reference point of filters</p>";
        
        popups.polarizedFilter = {};
        popups.polarizedFilter.image = "images/360labs/polarized-filter.jpg";
        popups.polarizedFilter.title = "Polarized filter tester";
        popups.polarizedFilter.content = "<p>To check if the filter is polarized filter. If the filter is polarized filter, an image of the tester is able to be observed through the filter.</p>";
        
        popups.polarizedAxisTwo = {};
        popups.polarizedAxisTwo.image = "images/360labs/polarized-axis-tester-2.jpg";
        popups.polarizedAxisTwo.title = "Polarized Axis tester";
        popups.polarizedAxisTwo.content = "<p>To test and determine the polarization axis of a single filter</p>";
        
        popups.multiExtractionHeating = {};
        popups.multiExtractionHeating.image = "images/360labs/multi-extraction-heating.jpg";
        popups.multiExtractionHeating.title = "Multi Extraction Heating Mantle";
        popups.multiExtractionHeating.content = "<p>Used to apply heat to boiling flasks in a controlled and safe manner. This unit is used to reflux (boil) materials in solvent for Indirect Food Additive testing. The condensers are affixed to the top of each boiling flask to collect solvent vapors and direct them back into the boiling flask.</p>";
        
        popups.fumeHoods = {};
        popups.fumeHoods.image = "images/360labs/fume-hoods.jpg";
        popups.fumeHoods.title = "Fume Hoods ";
        popups.fumeHoods.content = "<p>Used to handle chemical preparation work for specimens. Chemicals are mixed with the specimen in a safety environment for the ventilation system at the top of fume hood and a sliding tempered-glass door.</p>";
        
        popups.electronicPipettes = {};
        popups.electronicPipettes.image = "images/360labs/electronic-pipettes.jpg";
        popups.electronicPipettes.title = "Electronic Pipettes";
        popups.electronicPipettes.content = "<p>Used to suck and eject chemicals with quantity in high precision mainly for organic chemicals testing such as phthalates content.</p>";
        
        popups.rotaryEvaporator = {};
        popups.rotaryEvaporator.image = "images/360labs/rotary-evaporator.jpg";
        popups.rotaryEvaporator.title = "Rotary Evaporator";
        popups.rotaryEvaporator.content = "<p>Used to remove solvents from specimen by evaporation. A motor unit rotates the evaporation flask containing the specimen. A vacuum system keeps reducing the pressure within the evaporator system with the vapor duct transmitting the vapor being drawn off the specimen. A heater fluid bath heats the specimen.</p>";

        popups.hz_torqueTest = {};
        popups.hz_torqueTest.image = "images/360labs/hz_torqueTest.png";
        popups.hz_torqueTest.title = "Torque Test";
        popups.hz_torqueTest.content = "<p>Any toy with a projection, part, or assembly that a child can grasp with at least the thumb and forefinger or the teeth shall be tested. Product is placed in torque gauge and torque in each direction (clockwise and counter clockwise) shall be applied. Torque definition: a turning or twisting force</p>";
        
        popups.hz_flexibilityTest = {};
        popups.hz_flexibilityTest.image = "images/360labs/hz_flexibilityTest.png";
        popups.hz_flexibilityTest.title = "Flexibility Test";
        popups.hz_flexibilityTest.content = "<p>'Flexure test' is for determining compliance for wires or rods used a flexible skeletons.</p>";
        
        popups.hz_compressionTest = {};
        popups.hz_compressionTest.image = "images/360labs/hz_compressionTest.png";
        popups.hz_compressionTest.title = "Compression test";
        popups.hz_compressionTest.content = "<p>Any area on the surface of a toy that is accessible to a child and inaccessible to flat surface contact during the impact test shall be subject to this test. Compression force of predetermined amount is placed on surface.</p>";
        
        popups.hz_impactTest = {};
        popups.hz_impactTest.image = "images/360labs/hz_impactTest.png";
        popups.hz_impactTest.title = "Impact Test";
        popups.hz_impactTest.content = "<p>These tests are intended to simulate situations in which possible damage can occur to a toy by reason of its falling from a crib, table, counter top, or other impact situations that may occur as a result of reasonable foreseeable abuse. (drop, tip over, and tumble test fall under this)</p>";
        
        popups.hz_flammability = {};
        popups.hz_flammability.image = "images/360labs/hz_flammability.png";
        popups.hz_flammability.title = "Flammability";
        popups.hz_flammability.content = "<p>Flammability can fall under many different categories. Flam 1610, flam or solids, tent flam etc etc. I would find out what that lab has capability for and just add bullet points for test names.</p>";
        
        popups.hz_incline = {};
        popups.hz_incline.image = "images/360labs/hz_incline.png";
        popups.hz_incline.title = "Incline";
        popups.hz_incline.content = "<p>Stability of ride on toys, and toy seats. Intended to minimize unexpected hazards that could be caused by a toy that can tip easily on an incline. Weight is placed on product which is on a predetermined include- record signed of tip over or stability issues.</p>";
        
        popups.hz_tumbleTest = {};
        popups.hz_tumbleTest.image = "images/360labs/hz_tumbleTest.png";
        popups.hz_tumbleTest.title = "Tumble Test";
        popups.hz_tumbleTest.content = "<p>Wheeled toys weighing more than 3lbs but not more than 10 lbs shall be tumbled down a flight of six steps with risers not less than 7in. high.</p>";
        
        popups.hz_kineticEnergyMeasurement = {};
        popups.hz_kineticEnergyMeasurement.image = "images/360labs/hz_kineticEnergyMeasurement.png";
        popups.hz_kineticEnergyMeasurement.title = "Kinetic Energy Measurement";
        popups.hz_kineticEnergyMeasurement.content = "<p>Used to measure the speed of projectile objects.</p>";
        
        popups.hz_dropTesting = {};
        popups.hz_dropTesting.image = "images/360labs/hz_dropTesting.png";
        popups.hz_dropTesting.title = "Drop Testing";
        popups.hz_dropTesting.content = "<p>Toys falling below a specific weight limit based on age range shall be dropped in different orientations onto a specific impact area.</p>";
        
        popups.hz_tentFlam = {};
        popups.hz_tentFlam.image = "images/360labs/hz_tentFlam.png";
        popups.hz_tentFlam.title = "Tent Flam";
        popups.hz_tentFlam.content = "<p>Flammability test that burns tent components. Test is conducted on the Outer material, the inner materials and the floor material.</p>";
        
        popups.hz_tensionTest = {};
        popups.hz_tensionTest.image = "images/360labs/hz_tensionTest.png";
        popups.hz_tensionTest.title = "Tension Test";
        popups.hz_tensionTest.content = "<p>Any toy with a projection, part, or assembly that a child can grasp with at least the thumb and forefinger or the teeth shall be tested. A clamp capable of applying a tension load to the component shall be applied and appropriate amount of force used to pull.</p>";

        popups.loadingSystem = {};
        popups.loadingSystem.image = "images/360labs/loading-system.jpg";
        popups.loadingSystem.title = "Loading System";
        popups.loadingSystem.content = "<p>Used to lift up a mass up to 1 ton for evaluating any situation happen of an item under overloading.</p>";

        popups.transportationPlatform = {};
        popups.transportationPlatform.image = "images/360labs/transportation-platform.jpg";
        popups.transportationPlatform.title = "ISTA-1A Transportation Platform";
        popups.transportationPlatform.content = "<p>Used to provide a vibrating surface simulating the situation during transportation. It is special designed for packaged-products in 150 lb (68kg) or less. The packaged-products are challenged their capability to withstand transport hazards.</p>";
