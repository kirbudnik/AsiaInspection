<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="initial-scale = 1.0,maximum-scale = 1.0" />
    <title>{% block title %}Asia Inspection{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{asset('/css/bootstrap.css')}}" rel="stylesheet"/>
        <link href="{{asset('/css/bootstrap-theme.css')}}" rel="stylesheet"/>
        <link href="{{asset('/css/thickbox.css')}}" rel="stylesheet"/>
        {% stylesheets filter="cssrewrite" filter='?uglifycss' 
            '@AIResponsiveBundle/Resources/public/css/buttons.css'
        %}
        <link rel="stylesheet" href="{{asset_url}}"/>
        {% endstylesheets %}
        {% stylesheets filter="scss" filter='?uglifycss' 
            '@AIResponsiveBundle/Resources/public/css/styles.scss'
        %}
        <link id="mainCSS" rel="stylesheet" href="{{asset_url}}"/>
        {% endstylesheets %}
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>

<body style="overflow-x:hidden;">
    <div class="popup">
        <form class="validate-form" id="popupEmailForm">
            <div style="padding-top:7px;" dir="ltr">
                <span style="font-size:13px;">{{popup.desc}}</span><br /><br />
                <input type="email" id="userEmail" name="userEmail" required style="width:256px" />
                <input type="hidden" id="validateErrorText" value="error" />
                <input type="hidden" id="id" value="{{report}}" />
                <div style="clear:both;"></div>
                <button class="btn btn-primary three-btn" id="popupSubmit" style="margin-top:20px;" type="submit">{{popup.submit}}</button>
            </div>
        </form>
    </div>
    {% block javascripts %}
        <script src="{{asset('/js/jquery-1.11.2.min.js')}}"></script>
        <script src="{{asset('/js/bootstrap.js')}}"></script>
        <script src="{{asset('/js/thickbox.js')}}"></script>
        {% javascripts 
            '@AIResponsiveBundle/Resources/public/js/custom.js' 
            filter='?uglifyjs2' 
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
    {% endblock %}
</body>
</html>
