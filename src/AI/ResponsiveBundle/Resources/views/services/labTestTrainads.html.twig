<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html >
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="initial-scale = 1.0,maximum-scale = 1.0" />
        <meta name="apple-itunes-app" content="app-id=589783645">
        <title>Lab Testing - Train Ads</title>
        {% if tracking.head is defined %} {{tracking.head|raw}} {% endif %}
        {% block stylesheets %}

            <link href="{{asset('/css/bootstrap.css')}}" rel="stylesheet"/>
            <link href="{{asset('/css/bootstrap-theme.css')}}" rel="stylesheet"/>
            <link href="{{asset('/css/thickbox.css')}}" rel="stylesheet"/>
            {%stylesheets filter="cssrewrite"
                '@AIResponsiveBundle/Resources/public/css/buttons.css'
                
            %}
            <link rel="stylesheet" href="{{asset_url}}"/>
            {%endstylesheets%}
            {%stylesheets filter="scss"
                '@AIResponsiveBundle/Resources/public/css/styles.scss'
            %}
            <link id="mainCSS" rel="stylesheet" href="{{asset_url}}"/>
            {%endstylesheets%}

        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <script src="/js/jquery-1.11.2.min.js"></script>
        <script src="/js/mbox.js"></script>
    </head>
    <body style="font-family:SimSun;overflow-x:hidden;" {%if app.request.locale == "ar"%} dir="rtl"{%endif%}>
        <div id="page_trainads" class="container">
           {%if app.request.locale == "ar"%}{% include('AIResponsiveBundle:Default:SubTemplates/navRTL.html.twig')%}{%else%} {% include('AIResponsiveBundle:Default:SubTemplates/nav.html.twig')%} {%endif%}
<div class="headerbox">
    <div class="container wrapper vcenter_parent" >
        <div class="vcenter_parent">
            <div class="col-xl-8 col-xl-offset-0  col-lg-7 col-lg-offset-1 col-xs-offset-1 col-xs-11 vcenter" style="padding-left:0px;">
                <h3 style="font-family:SimSun;"class="mainHeader">{{xml.title}}</h3>      
                 <div class="mainSubHeader">{{xml.subtext|raw}}</div>   
            </div>
            <div class=" col-xs-offset-1 col-xs-10 col-sm-offset-0  col-sm-5 col-md-4 col-md-offset-0 col-lg-4 col-lg-offset-0 right text-center vcenter displayNone1200" >
                
                <div class="sidebarTitlebar">
                    <h5>{{xml.formTxt|raw}}</h5>
                </div>
                <div class="sidebarContentBox">
                    <form  class="validate-form" id="consultForm">
                     <input type="text" id="consultFormName" style="width:100%;margin-bottom:10px;height:28px;font-family:SimSun;" required placeholder="{{xml.formName}}">
                     <input type="email" id="consultFormEmail" style="width:100%;margin-bottom:10px;height:28px;font-family:SimSun;" required placeholder="{{xml.formEmail}}">
                     <input type="text" id="consultFormPhone" style="width:100%;margin-bottom:10px;height:28px;font-family:SimSun;"required placeholder="{{xml.formPhone}}">
                     <button type="button" id="consultFormBtn" class="btn-primary" style="width:100%;">{{xml.formGetInfo}}</button>
                    </form>
                    <p id="submitFormSuccess" style="display:none;font-family:SimSun;">{{xml.formSubmitted}}</p>
                </div>

            </div>
        </div>
    </div>
</div>

<div class="container wrapper mainBlock" >
    <div class="col-xs-12 subMainBlock">
        <div class="col-xs-12 col-sm-offset-1 col-sm-6 col-md-offset-1 col-md-7 col-lg-7 col-xl-offset-0 col-xl-8 sidebox " >

             <h2 class="marginZero padding15" style="font-weight:bold;font-family:SimSun;">{{xml.WhyCompliance.title}}</h2>
                {{xml.WhyCompliance.text|raw}}

            {%for section in xml.sections.section%}
              <h2 class="marginZero padding15" style="font-weight:bold;font-family:SimSun;">{{section.title}}</h2>
                <div class="marginZero " style="padding-bottom:5px;">
                    <ul class="marginZero">
                      {%for list in section.List.li%}
                        <li style="font-family:SimSun;">{{list|raw}}</li>
                      {%endfor%}
                    </ul>
                  </div>
            {%endfor%}
        </div>

        <div class="sidebox col-xs-12 col-sm-offset-0  col-sm-5 col-md-4 col-md-offset-0 col-lg-4 col-lg-offset-0" >
             <div class="sidebox  col-xs-12 col-md-offset-1 text-center display1200" style="margin-top:15px;margin-bottom:15px;display:none;">
                
                <div class="sidebarTitlebar">
                    <h5>{{xml.formTxt|raw}}</h5>
                </div>
                <div class="sidebarContentBox">
                    <form  class="validate-form" id="consultFormSm">
                     <input type="text" id="consultFormNameSm" style="width:100%;margin-bottom:10px;height:28px;font-family:SimSun;" required placeholder="{{xml.formName}}">
                     <input type="email" id="consultFormEmailSm" style="width:100%;margin-bottom:10px;height:28px;font-family:SimSun;" required placeholder="{{xml.formEmail}}">
                     <input type="text" id="consultFormPhoneSm" style="width:100%;margin-bottom:10px;height:28px;font-family:SimSun;" required placeholder="{{xml.formPhone}}">
                     <button type="button" id="consultFormBtnSm" class="btn-primary" style="width:100%;font-family:SimSun;">{{xml.formGetInfo}}</button>
                    </form>
                    <p id="submitFormSuccessSm" style="display:none;font-family:SimSun;">{{xml.formSubmitted}}</p>
                </div>

            </div>
            <div class="sidebox col-xs-12 col-md-offset-1">

                <div class="sidebarTitlebar">
                    <h5>{{xml.contactUs}}</h5>
                </div>
                <div class="sidebarContentBox">
                    {{xml.offices|raw}}
                    <div class="col-xs-12 " id="sidebox-downSample" >
                        <div class="col-xs-1 paddingLeftZero"  >
                            <img src="https://s3.asiainspection.com/images/responsive/downSample.png" width="30px">
                        </div>
                        <div class="col-xs-11 " style="padding-right:0px;">
                            <button title="{% trans %}Download Sample Report{% endtrans %}" id="inspection-services-links-1-us" style="color:#337ab7; font-size:12px;" class="serviceReport" type="labtest" report="lab-testing">{% trans %}Download a Sample Report{% endtrans %}</button>
                        </div>
                    </div> 
                     <div class="col-xs-12 " id="sidebox-downSample" style="margin-top:10px;">
                        <div class="col-xs-1 paddingLeftZero"  >
                            <img src="https://s3.asiainspection.com/images/responsive/downSample.png" width="30px">
                        </div>
                        <div class="col-xs-11 " style="padding-right:0px;padding-top:7px;">
                            <a href="mailto:chbsales@asiainspection.com" style="font-family:SimSun">{{xml.getQuote}}</a>
                        </div>
                    </div> 
                    <div style="clear:both;"></div>
                </div>

            </div>            
        </div>
    </div>
</div>
  {% include('AIResponsiveBundle:Default:SubTemplates/footerCHB.html.twig') %}
        </div>
        {% block javascripts %}
            <script src="{{asset('/js/bootstrap.js')}}"></script>
            <script src="{{asset('/js/thickbox.js')}}"></script>
            <script src="{{asset('/js/jquery.mousewheel.min.js')}}"></script>
            <script src="{{asset('/js/unslider.min.js')}}"></script>
            {% javascripts 
                '@AIResponsiveBundle/Resources/public/js/custom.js' 
                filter='?uglifyjs2' 
            %}
            <script type="text/javascript" src="{{ asset_url }}"></script>
            {% endjavascripts %}
        {% endblock %}

        {% if tracking.body is defined %} {{tracking.body|raw}} {% endif %}

        <div id="backToTop"><img src="/images/up_chevron.png" style="height:15px; max-height:100%; padding-right:10px;">Back to Top</div>
    </body>
</html>
