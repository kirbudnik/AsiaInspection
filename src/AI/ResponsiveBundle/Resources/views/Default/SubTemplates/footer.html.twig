{% if app.request.cookies.get('marketo') is not null and "emailPreferences" not in app.request.pathinfo and "register" not in app.request.pathinfo and "registrationsuccess" not in app.request.pathinfo and "mobile-regulatory-updates" not in app.request.pathinfo %}
    <script type="text/javascript">
    $(document).ready(function(){
        if(typeof tb_show === "function") {
            marketo = getCookie("marketo");
            console.log(marketo);
            marketo = JSON.parse(marketo)[0];
            // -- Debug --
            console.log("\n-- Debug --");
            console.log("Email: "+marketo.Email);
            if(marketo.activeConsentDate == undefined) { console.log("Active Consent: NEED"); } else { console.log("Active Consent: HAVE"); }
            if(marketo.isEU == "1") { console.log("European: YES"); } else { console.log("European: NO"); }
            if(marketo.clientStatus == "Non-Opt In List" || marketo.leadStatus == "Non-Opt In List") { console.log("Opt-In: YES"); } else { console.log("Opt-In: NO"); }
            if(marketo.clientStatus == "Employee" || marketo.leadStatus == "Employee") { console.log("Employee: YES"); } else { console.log("Employee: NO"); }
            if(marketo.clientStatus == "Do Not Disturb" || marketo.leadStatus == "Do Not Disturb") { console.log("Do Not Disturb: YES"); } else { console.log("Do Not Disturb: NO"); }
            if(getCookie("showGDPRpopup") != 1) { console.log("SKIP: NO"); } else { console.log("SKIP: YES"); }
            console.log("-- Debug --\n");
            // -- Debug --
            if(marketo.activeConsentDate == undefined && marketo.isEU == "1" && marketo.clientStatus != "Non-Opt In List" && marketo.clientStatus != "Employee" && marketo.clientStatus != "Do Not Disturb" && marketo.leadStatus != "Non-Opt In List" && marketo.leadStatus != "Employee" && marketo.leadStatus != "Do Not Disturb" && getCookie("showGDPRpopup") != 1) {
                setTimeout(function() {
                    tb_show("Action Required for "+marketo.Email,"/popup/gdpr-consent/"+marketo.Email);
                    $("#TB_ajaxContent").css({
                        "height":"auto",
                        "width":"auto"
                    });
                }, 5000);
            }
        }
    });
    </script>

