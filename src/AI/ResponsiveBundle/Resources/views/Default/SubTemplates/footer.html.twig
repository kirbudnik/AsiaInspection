<script type="text/javascript">
    function gup() {
        var qs = document.location.search;
        qs = qs.split('+').join(' ');
        var params = {}, tokens, re = /[?&]?([^=]+)=([^&]*)/g;
        while (tokens = re.exec(qs))
            params[decodeURIComponent(tokens[1])] = decodeURIComponent(tokens[2]);
        return params;
    }
    var params = gup()
    if(params.debug != undefined){
        // Popup Settings
        html = '<div style="position:absolute; top:20px; right:20px; min-width:300px; min-height:300px; background-color:#DADFE5; border:1px dashed black; text-align:left; padding:15px; z-index:3000; box-shadow: -10px 10px 5px 0px rgba(0,0,0,0.75);">';
        html += '<b>GDPR Cookie Settings</b><br /><br />';
        html += '<input id="chk_Consent" type="checkbox" style="-ms-transform: scale(2); -moz-transform: scale(2); -webkit-transform: scale(2); -o-transform: scale(2);"> &nbsp; Consent<br /><br />';
        html += '<input id="chk_EU" type="checkbox" style="-ms-transform: scale(2); -moz-transform: scale(2); -webkit-transform: scale(2); -o-transform: scale(2);"> &nbsp; European<br /><br />';
        html += 'Client Status:<br />';
        html += '<input id="chk_client_nonopt" type="checkbox" style="-ms-transform: scale(2); -moz-transform: scale(2); -webkit-transform: scale(2); -o-transform: scale(2);"> &nbsp; Non-Opt In<br />';
        html += '<input id="chk_client_emp" type="checkbox" style="-ms-transform: scale(2); -moz-transform: scale(2); -webkit-transform: scale(2); -o-transform: scale(2);"> &nbsp; Employee<br />';
        html += '<input id="chk_client_dnd" type="checkbox" style="-ms-transform: scale(2); -moz-transform: scale(2); -webkit-transform: scale(2); -o-transform: scale(2);"> &nbsp; Do Not Disturb<br /><br />';
        html += 'Lead Status:<br />';
        html += '<input id="chk_lead_nonopt" type="checkbox" style="-ms-transform: scale(2); -moz-transform: scale(2); -webkit-transform: scale(2); -o-transform: scale(2);"> &nbsp; Non-Opt In<br />';
        html += '<input id="chk_lead_emp" type="checkbox" style="-ms-transform: scale(2); -moz-transform: scale(2); -webkit-transform: scale(2); -o-transform: scale(2);"> &nbsp; Employee<br />';
        html += '<input id="chk_lead_dnd" type="checkbox" style="-ms-transform: scale(2); -moz-transform: scale(2); -webkit-transform: scale(2); -o-transform: scale(2);"> &nbsp; Do Not Disturb<br /><br />';
        html += '<input id="chk_deleteSkip" type="checkbox" style="-ms-transform: scale(2); -moz-transform: scale(2); -webkit-transform: scale(2); -o-transform: scale(2);"> &nbsp; Delete Skip Cookie?<br />';
        html += '<br /><a href="javascript:void(0)" onClick="doTheDew();">Reload Page</a>';
        html += '</div>';
        $("body").append(html);
    }
    function doTheDew() {
        ck_consent = ($("#chk_Consent").prop("checked") ? "2018-03-08" : "");
        ck_eu = ($("#chk_EU").prop("checked") ? "1" : "");
        ck_client = "";
        if($("#chk_client_nonopt").prop("checked")) ck_client = "Non-Opt In List";
        if($("#chk_client_emp").prop("checked")) ck_client = "Employee";
        if($("#chk_client_dnd").prop("checked")) ck_client = "Do Not Disturb";
        ck_lead = "";
        if($("#chk_lead_nonopt").prop("checked")) ck_lead = "Non-Opt In List";
        if($("#chk_lead_emp").prop("checked")) ck_lead = "Employee";
        if($("#chk_lead_dnd").prop("checked")) ck_lead = "Do Not Disturb";
        if($("#chk_deleteSkip").prop("checked")) setCookie("showGDPRpopup",0,0);
        //coooookey = '[{"Id":149702,"Email":"vincent.macdonald@asiainspection.com"'+ck_consent+'","clientStatus":"'+ck_client+'","leadStatus":"'+ck_lead+'","FirstName":"Vincent","LastName":"Macdonald","isEU":"'+ck_eu+'"}]';
        coooookey = {"Id":149702, "Email":"vincent.macdonald@asiainspection.com", "clientStatus":ck_client, "leadStatus":ck_lead, "FirstName":"Vincent", "LastName":"Macdonald", "isEU":ck_eu};
        if(ck_consent != "") coooookey.activeConsentDate = ck_consent;
        coooookey = "["+JSON.stringify(coooookey)+"]";
        setCookie("marketo",coooookey,1);
        window.location = window.location.href.split("?")[0];
    }
</script>
